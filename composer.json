{
    "name": "soap/jongman-domain",
    "description": "Business domain classes for JONGman reservation package.",
    "keywords": [
        "soap",
        "jongman",
        "reservation"
    ],
    "homepage": "https://github.com/soap/jongman-domain",
    "license": "MIT",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Soap\\Jongman\\Domain\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\Jongman\\Domain\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
